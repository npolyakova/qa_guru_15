{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "User info",
  "type": "object",
  "data": {
    "title": "Data",
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "email": {
        "type": "string"
      },
      "first_name": {
        "type": "string"
      },
      "last_name": {
        "type": "string"
      },
      "avatar": {
        "type": "string"
      }
    },
    "support": {
      "title": "Support info",
      "type": "object",
      "properties": {
        "url": {
          "type": "string"
        },
        "text": {
          "type": "string"
        }
      }
  }
}
}